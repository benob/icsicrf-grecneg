<REFEX ENTITY="0" REG08-TYPE="name" CASE="plain" TAG="plain_name">John Jenkins</REFEX>
<REFEX ENTITY="0" REG08-TYPE="name" CASE="plain" TAG="plain_name">the aged Jenkins</REFEX>
